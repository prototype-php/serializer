{
    "name": "prototype/serializer",
    "description": "Protobuf serialization without an inheritance.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "protobuf",
        "reflection"
    ],
    "authors": [
        {
            "name": "kafkiansky",
            "email": "vadimzanfir@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "prototype/byte": "*",
        "psr/simple-cache": "^3.0",
        "typhoon/reflection": "^0.4.0",
        "typhoon/typed-map": "^0.4.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Prototype\\Serializer\\": ""
        },
        "files": [
            "Internal/Wire/functions.php",
            "Internal/Reflection/functions.php"
        ]
    },
    "config": {
        "lock": false,
        "sort-packages": true
    }
}
