{
    "name": "prototype/serializer",
    "description": "Protobuf serialization without an inheritance.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "protobuf",
        "reflection"
    ],
    "authors": [
        {
            "name": "kafkiansky",
            "email": "vadimzanfir@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "kafkiansky/binary": "^0.4.4",
        "psr/simple-cache": "^3.0@dev",
        "typhoon/reflection": "^0.4.x-dev",
        "typhoon/typed-map": "0.4.x-dev"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.43",
        "friendsofphp/php-cs-fixer": "^3.59",
        "jetbrains/phpstorm-stubs": "^2024.1",
        "phpunit/phpunit": "^10.5",
        "psalm/plugin-phpunit": "^0.19.0",
        "symfony/var-dumper": "^6.4",
        "vimeo/psalm": "^5.25"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Prototype\\Serializer\\": ""
        },
        "files": [
            "Internal/Wire/functions.php",
            "Internal/Reflection/functions.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "lock": false
    }
}
